{
  "name": "maybes",
  "version": "0.1.11",
  "description": "Maybe is a type that wraps optional values",
  "keywords": [
    "maybe",
    "optional",
    "option",
    "just",
    "nothing",
    "some",
    "none"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alexanderjarvis/maybe.git"
  },
  "author": "Alexander Jarvis",
  "license": "MIT",
  "main": "lib/maybe.js",
  "scripts": {
    "flow": "flow",
    "lint": "eslint . --max-warnings 0",
    "test": "jest",
    "compile": "babel src --out-dir lib --ignore src/maybe.test.js && cp src/maybe.js.flow lib",
    "prepublish": "yarn run compile",
    "check": "yarn run lint && yarn run flow check && yarn test"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.39.0",
    "jest": "^18.1.0"
  }
}
